{
  "name": "AddBookmark",
  "version": "0.1",
  "manifest_version": 2,
  "description": "A smarter 'Add Bookmark' button",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://*/*","https://*/*", "bookmarks", "storage"
  ],
  "background": {
      "scripts": ["coffee-classifier/Porter-Stemmer/PorterStemmer1980.min.js",
                  "naivebayes.js", "eventPage.js"],
      "persistent": false
  }
}
